<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8dp"/>
                <gradient android:startColor="#ff7494ed" android:endColor="#06101d" android:angle="0" android:centerY="1" android:centerColor="#ff8a7cb7"/>
            </shape>
        </clip>
    </item>
</layer-list>
